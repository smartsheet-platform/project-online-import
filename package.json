{
  "name": "project-online-import",
  "version": "1.0.0",
  "description": "CLI tool for importing Project Online data to Smartsheet",
  "main": "dist/index.js",
  "bin": {
    "project-online-import": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "bash scripts/run-integration-tests.sh",
    "test:integration:debug": "CLEANUP_TEST_WORKSPACES=false bash scripts/run-integration-tests.sh",
    "test:unit": "jest --testPathIgnorePatterns=test/integration",
    "test:connection": "ts-node scripts/test-project-online-connection.ts",
    "test:rest-api": "ts-node scripts/test-rest-api-alternatives.ts",
    "test:http": "ts-node scripts/test-raw-http-request.ts",
    "diagnose:pwa": "ts-node scripts/diagnose-pwa-instance.ts",
    "diagnose:token": "ts-node scripts/diagnose-token-scopes.ts",
    "diagnose:permissions": "ts-node scripts/diagnose-project-online-permissions.ts",
    "cleanup-test-workspaces": "ts-node scripts/cleanup-test-workspaces.ts",
    "create-sample-workspace": "ts-node scripts/create-sample-workspace.ts",
    "extract-real-data": "ts-node scripts/extract-real-project-data.ts",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'test/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "cli",
    "project-online",
    "smartsheet",
    "import",
    "etl",
    "migration",
    "microsoft-project",
    "project-management",
    "typescript"
  ],
  "author": "Smartsheet",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/smar-shikhar-krishna/project-online-import.git"
  },
  "bugs": {
    "url": "https://github.com/smar-shikhar-krishna/project-online-import/issues"
  },
  "homepage": "https://github.com/smar-shikhar-krishna/project-online-import#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "axios": "^1.6.7",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "smartsheet": "^4.7.0",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
